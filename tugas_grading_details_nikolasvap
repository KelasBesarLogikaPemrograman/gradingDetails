```python
print("LOGIN PAGE")
print()
print("Type in 'enter()' to continue")

username_1 = "Yulizar"
password_1 = "UPNVJ2022"
username_2 = "Lilik"
password_2 = "2022UPNVJ"
username_3 = "Halim"
password_3 = "UPN2022VJ"

def enter() :
    enter_input = input("Type 'Login' to enter login info : ")
    if enter_input == "Login" :
        enter_answer = login()
        return enter_answer
    else :
        enter_answer = "Denied Access"
        return enter_answer

def login() :
    username = input("Input a username : ")
    password = input("Input password : ")
    if username == username_1 and password ==password_1 or username == username_2 and password == password_2 or username == username_3 and password == password_3:
        print("status_login = active")
        print("Halo, " + username)
        login_answer = score()
        return login_answer
    else :
        login_answer = "Denied Access"
        return login_answer

def score() :
    nama_mahasiswa = input("Masukkan Nama Mahasiswa yang akan di grading : ")
    nama_matkul = input("Nama Mata Kuliah : ")
    nilai_presensi = input("Silahkan Masukkan nilai Presensi : ")
    nilai_tugas = input("Silahkan Masukkan nilai Tugas : ")
    nilai_uts = input("Silahkan Masukkan nilai UTS : ")
    nilai_uas = input("Silahkan Masukkan nilai UAS : ")
    score_array = [nama_mahasiswa, nama_matkul, nilai_presensi, nilai_tugas, nilai_uts, nilai_uas]
    total_nilai = (0.05 * float(score_array[2])) + (0.1 * float(score_array[3])) + (0.30 * float(score_array[3])) + (0.25 * float(score_array[4])) + (0.30 * float(score_array[5]))
    print (score_array)
    if total_nilai <= 100 :
        score_sum = "A"
    elif total_nilai <= 84 :
        score_sum = "B"
    elif total_nilai <= 69 :
        score_sum = "C"
    elif total_nilai <= 49 :
        score_sum = "D"
    else :
        score_sum = "E"
    print("Mahasiswa", nama_mahasiswa, "mendapatkan nilai", score_sum,  "untuk Mata Kuliah", nama_matkul, "dengan skor total", total_nilai)
